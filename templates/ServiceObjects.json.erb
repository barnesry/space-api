{
  "SERVICE_OBJECTS": {
<% services_only.each do | service | %>
    "<%= service.name %>": {
<% if !service.protocols.empty? %>
      "PROTOCOL": "<% service.protocols.find do |protocol| %><%= protocol.get_protocol_type() %><% end %>",
      "DST_PORT": "<% service.protocols.find do |protocol| %><%= protocol.dst_port %><% end %>",
<% end %>
      "DESCRIPTION": "<%= service.description %>"
    },
<% end %>
  }
}